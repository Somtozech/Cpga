{
  "name": "cpga",
  "version": "0.1.0",
  "private": true,
  "main": "./public/electron.js",
  "dependencies": {
    "electron": "^5.0.2",
    "electron-is-dev": "^1.1.0",
    "is-electron": "^2.2.0",
    "lowdb": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "shortid": "^2.2.14",
    "xlsx": "^0.14.3"
  },
  "build": {
    "appId": "com.electron.cpga",
    "productName": "Cpga",
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"  ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "yarn start && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^20.43.0"
  }
}
