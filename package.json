{
  "name": "transkript",
  "version": "0.4.0",
  "private": true,
  "description": "Transkript  is a software application used for result processing and transcript generation",
  "author": "Ezechi Nnaemeka Somtochukwu",
  "main": "./public/electron.js",
  "homepage": ".",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
